{

  "family": "my-task-family",

  "networkMode": "awsvpc",

  "executionRoleArn": "arn:aws:iam::095927792931:role/myTaskExecutionRole",

  "containerDefinitions": [

    {

      "name": "my-container",

      "image": "placeholder",

      "essential": true,

      "portMappings": [

        { "containerPort": 80, "hostPort": 80 }

      ],

      "logConfiguration": {

        "logDriver": "awslogs",

        "options": {

          "awslogs-group": "/ecs/my-task-family",

          "awslogs-region": "us-east-1",

          "awslogs-stream-prefix": "ecs"

        }

      }

    }

  ],

  "requiresCompatibilities": ["FARGATE"],

  "cpu": "256",

  "memory": "512"

}

 
